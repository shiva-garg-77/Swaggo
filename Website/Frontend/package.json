{
  "name": "swaggo-frontend",
  "version": "1.0.0",
  "sideEffects": false,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "node scripts/dev-flexible.js",
    "dev:3000": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && set NEXT_WEBPACK_USEPOLLING=false && set NEXT_TELEMETRY_DISABLED=1 && next dev -p %PORT% --hostname localhost",
    "dev:clean": "copy .env.development.clean .env.development.local && set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && set NEXT_TELEMETRY_DISABLED=1 && next dev -p %PORT% --hostname localhost",
    "dev:super-clean": "powershell -ExecutionPolicy Bypass -File dev-clean.ps1",
    "dev:windows:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:stable-windows": "copy .env.development.windows .env.development.local && npm run dev:windows",
    "dev:hmr": "node scripts/dev-hmr-optimized.js",
    "dev:ultra-performance": "node scripts/ultra-performance-startup.js",
    "dev:ultra-stable": "node scripts/ultra-stable-startup.js",
    "dev:crash-free": "node scripts/crash-free-startup.js",
    "dev:clean-restart": "node scripts/clean-restart.js",
    "build": "next build",
    "build:fast": "SKIP_LINT=true next build",
    "build:optimize": "node scripts/bundle-optimizer.js && NODE_ENV=production next build",
    "build:analyze": "node scripts/bundle-optimizer.js && next build",
    "optimize": "node scripts/bundle-optimizer.js",
    "optimize:fix": "node scripts/bundle-optimizer.js --fix",
    "start": "next start -p 3000",
    "start:3000": "next start -p 3000",
    "start:3001": "next start -p 3001",
    "start:production": "NODE_ENV=production next start -p 3000",
    "analyze": "set \"ANALYZE=true\" && next build",
    "analyze:bundle": "node scripts/bundle-analyzer.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue .next, node_modules\\.cache\"",
    "clean:build": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue .next\"",
    "clean:cache": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue node_modules\\.cache\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "test:integration": "jest --testPathPatterns=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:audit": "jest --testPathPattern=audit",
    "test:all": "npm run test:security && npm run test:integration && npm run test:performance && npm run test:audit",
    "security:validate": "node scripts/security-validation.js",
    "security:scan": "node scripts/security-scan.js",
    "security-audit": "npm audit --audit-level=high",
    "security-fix": "npm audit fix",
    "debug:connection": "node scripts/debug-connection.js"
  },
  "dependencies": {
    "@apollo/client": "^3.14.0",
    "@emotion/is-prop-valid": "^1.4.0",
    "@next/bundle-analyzer": "^15.5.4",
    "@swc/core": "^1.13.21",
    "@swc/helpers": "^0.5.17",
    "@tailwindcss/postcss": "^4.1.14",
    "@wry/caches": "^1.0.1",
    "@wry/context": "^0.7.4",
    "@wry/equality": "^0.5.7",
    "@wry/trie": "^0.5.0",
    "autoprefixer": "^10.4.21",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "dayjs": "^1.11.18",
    "framer-motion": "^12.23.24",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "immer": "^10.1.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "optimism": "^0.18.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "rxjs": "^7.8.2",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "ts-invariant": "^0.10.3",
    "zen-observable-ts": "^1.2.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "19.2.2",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.4",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "prettier": "^3.4.2",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "graphql": "16.11.0"
  }
}
