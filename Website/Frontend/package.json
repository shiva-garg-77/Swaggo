{
  "name": "swaggo-web",
  "version": "0.1.0",
  "sideEffects": false,
  "scripts": {
    "dev": "node scripts/ultra-performance-startup.js",
    "dev:hmr": "node scripts/dev-hmr-optimized.js",
    "dev:ultra-performance": "node scripts/ultra-performance-startup.js",
    "dev:ultra-stable": "node scripts/ultra-stable-startup.js",
    "dev:legacy": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:crash-free": "node scripts/crash-free-startup.js",
    "dev:safe": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:clean-restart": "node scripts/clean-restart.js",
    "dev:3000": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:normal": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:normal:3000": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:normal:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:stable": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:stable:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:windows": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3000 --hostname localhost",
    "dev:windows:3001": "set NODE_OPTIONS=--max-old-space-size=4096 --no-warnings && next dev -p 3001 --hostname localhost",
    "dev:stable-windows": "copy .env.development.windows .env.development.local && npm run dev:windows",
    "build": "next build",
    "build:fast": "SKIP_LINT=true next build",
    "build:optimize": "node scripts/bundle-optimizer.js && NODE_ENV=production next build",
    "build:analyze": "ANALYZE=true next build",
    "optimize": "node scripts/bundle-optimizer.js",
    "optimize:fix": "node scripts/bundle-optimizer.js --fix",
    "start": "next start -p 3000",
    "start:3000": "next start -p 3000",
    "start:3001": "next start -p 3001",
    "start:production": "NODE_ENV=production next start -p 3000",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force -ErrorAction SilentlyContinue .next, node_modules\\.cache\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest --testPathPattern=security",
    "test:integration": "jest --testPathPatterns=integration",
    "test:performance": "jest --testPathPattern=performance",
    "test:audit": "jest --testPathPattern=audit",
    "test:all": "npm run test:security && npm run test:integration && npm run test:performance && npm run test:audit",
    "security:validate": "node scripts/security-validation.js",
    "security:scan": "node scripts/security-scan.js",
    "debug:connection": "node scripts/debug-connection.js"
  },
  "dependencies": {
    "@apollo/client": "^4.0.6",
    "@babel/core": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.8",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.90.2",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "buffer": "^6.0.3",
    "critters": "^0.0.23",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.18",
    "dotenv": "^17.2.2",
    "framer-motion": "^12.23.12",
    "graphql": "^16.11.0",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.543.0",
    "next": "^15.5.4",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.6.0",
    "rxjs": "^7.8.2",
    "scheduler": "^0.26.0",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.19",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "uuid": "^13.0.0",
    "vite": "^7.1.7",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/runtime": "^7.28.4",
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@next/bundle-analyzer": "^15.5.4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.5.2",
    "@types/react": "^18.3.24",
    "compression-webpack-plugin": "^11.0.0",
    "eslint": "^9.36.0",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "process": "^0.11.10",
    "react-refresh": "^0.17.0",
    "terser-webpack-plugin": "^5.3.14",
    "typescript": "^5.9.2",
    "util": "^0.12.5",
    "web-push": "^3.6.7",
    "webpack-bundle-analyzer": "^4.10.2",
    "whatwg-fetch": "^3.6.20"
  }
}
