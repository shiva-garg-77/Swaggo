{
  "name": "swaggo-backend",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "description": "Swaggo Backend with 10/10 Secure Authentication System",
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:auth": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/auth",
    "test:token-rotation": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/auth/TokenRotation.test.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/auth/Integration.test.js",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/auth/E2E.test.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:security": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/auth --coverage",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --watchAll=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint:check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "docs": "jsdoc -c jsdoc.config.json",
    "docs:md": "jsdoc2md --configure jsdoc.config.json --files '**/*.js' --template docs/template.hbs > docs/API.md",
    "docs:serve": "npx http-server docs -p 3002",
    "type-check": "tsc --noEmit --allowJs --checkJs",
    "analyze": "nyc report --reporter=text-summary",
    "security-audit": "npm audit --audit-level=high",
    "security-check": "npm run security-audit && npm run lint:check",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "npm run quality:fix && npm run test:ci && npm run security-check",
    "dev:setup": "node scripts/dev.js setup",
    "dev:validate": "node scripts/dev.js validate",
    "dev:reset-db": "node scripts/dev.js reset-db",
    "dev:test": "node scripts/dev.js test",
    "dev:docs": "node scripts/dev.js docs",
    "dev:clean": "node scripts/dev.js clean"
  },
  "keywords": [
    "authentication",
    "security",
    "jwt",
    "express",
    "mongodb",
    "graphql",
    "socket.io"
  ],
  "author": "Swaggo Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@simplewebauthn/browser": "^13.2.0",
    "@simplewebauthn/server": "^13.2.1",
    "apollo-server-express": "^3.13.0",
    "argon2": "^0.44.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "csrf": "^3.1.0",
    "dataloader": "^2.2.3",
    "dayjs": "^1.11.18",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "geoip-lite": "^1.4.10",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.27.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.18.2",
    "multer": "^1.4.5-lts.2",
    "node-device-detector": "^2.2.4",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "qrcode": "^1.5.4",
    "randomstring": "^1.3.1",

    "snappy": "^7.3.3",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ua-parser-js": "^2.0.5",
    "uuid": "^11.1.0",
    "validator": "^13.15.15",
    "web-push": "^3.6.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^48.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.24.0",
    "husky": "^9.0.0",
    "jest": "^30.1.3",
    "jest-html-reporters": "^3.1.7",
    "jest-junit": "^16.0.0",
    "jsdoc": "^4.0.0",
    "jsdoc-to-markdown": "^8.0.0",
    "lint-staged": "^15.0.0",
    "mongodb-memory-server": "^10.2.1",
    "nyc": "^15.1.0",
    "prettier": "^3.2.0",
    "puppeteer": "^22.6.5",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.4",
    "typescript": "^5.3.0"
  }
}
