{
  "name": "swaggo-backend",
  "version": "1.0.0",
  "main": "main.js",
  "type": "module",
  "description": "Swaggo Backend with 10/10 Secure Authentication System",
  "scripts": {
    "start": "node main.js",
    "dev": "nodemon main.js",
    "build": "tsc",
    "test": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js",
    "test:auth": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js tests/auth",
    "test:token-rotation": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js tests/auth/TokenRotation.test.js",
    "test:integration": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js tests/auth/Integration.test.js",
    "test:e2e": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js tests/auth/E2E.test.js",
    "test:watch": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js --watch",
    "test:security": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js tests/auth --coverage",
    "test:coverage": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js --coverage",
    "test:automated": "node tests/automated-test-runner.js",
    "test:automated:watch": "node tests/automated-test-runner.js --watch",
    "test:helpers": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js Helper/__tests__",
    "test:run-automated": "node tests/run-automated-tests.js",
    "test:ci": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js --ci --coverage --watchAll=false",
    "test:unit": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit",
    "test:unit:config": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit/config",
    "test:unit:middleware": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit/middleware",
    "test:unit:utils": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit/utils",
    "test:unit:services": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit/services",
    "test:unit:models": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/unit/models",
    "test:performance": "node --experimental-vm-modules ../../node_modules/jest-cli/bin/jest.js __tests__/performance",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:check": "eslint . --max-warnings 0",
    "lint:security": "eslint . --config .eslintrc.security.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "quality": "npm run lint:check && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "quality:evaluate": "node scripts/evaluate-quality-gates.js",
    "docs": "jsdoc -c jsdoc.config.json",
    "docs:md": "jsdoc2md --configure jsdoc.config.json --files '**/*.js' --template docs/template.hbs > docs/API.md",
    "docs:serve": "npx http-server docs -p 3002",
    "type-check": "tsc --noEmit --allowJs --checkJs",
    "analyze": "nyc report --reporter=text-summary",
    "analyze:coverage": "nyc report --reporter=html --reporter=text",
    "analyze:complexity": "node scripts/analyze-complexity.js",
    "security-audit": "npm audit --audit-level=high",
    "security-check": "npm run security-audit && npm run lint:check",
    "security-fix": "npm audit fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "validate": "npm run quality:fix && npm run test:ci && npm run security-check",
    "dev:setup": "node scripts/dev.js setup",
    "dev:validate": "node scripts/dev.js validate",
    "dev:reset-db": "node scripts/dev.js reset-db",
    "dev:test": "node scripts/dev.js test",
    "dev:docs": "node scripts/dev.js docs",
    "dev:clean": "node scripts/dev.js clean",
    "secrets:migrate": "node Scripts/migrateSecrets.js",
    "secrets:validate": "node Scripts/SecretValidator.js",
    "secrets:generate": "node Scripts/SecretValidator.js generate",
    "setup:env": "node Scripts/SecretValidator.js generate",
    "setup:test-env": "node scripts/setup-test-env.js",
    "setup:integration-env": "node scripts/setup-integration-env.js",
    "setup:performance-env": "node scripts/setup-performance-env.js",
    "clean": "rimraf dist coverage .nyc_output",
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "clean:logs": "rimraf logs/*.log",
    "db:seed": "node Scripts/seed-database.js",
    "db:migrate": "node scripts/migrate-database.js",
    "db:reset": "node scripts/reset-database.js",
    "db:test-setup": "node scripts/setup-test-db.js",
    "db:performance-setup": "node scripts/setup-performance-db.js",
    "migrate": "node Scripts/run-migrations.js up",
    "migrate:down": "node Scripts/run-migrations.js down",
    "migrate:create": "node Scripts/run-migrations.js create",
    "backup:create": "node scripts/create-backup.js",
    "backup:restore": "node scripts/restore-backup.js",
    "backup:list": "node scripts/list-backups.js",
    "build:test": "tsc --project tsconfig.test.json",
    "start:test": "node dist/test-server.js",
    "coverage:check": "node scripts/check-coverage.js",
    "coverage:merge": "node scripts/merge-coverage.js",
    "coverage:report": "nyc report",
    "report:comprehensive": "node scripts/generate-comprehensive-report.js",
    "test:report:integration": "node scripts/generate-integration-report.js",
    "test:e2e:report": "node scripts/generate-e2e-report.js",
    "test:e2e:screenshots": "node scripts/process-e2e-screenshots.js",
    "performance:report": "node scripts/generate-performance-report.js",
    "performance:benchmark": "node scripts/run-benchmark.js",
    "performance:compare": "node scripts/compare-performance.js",
    "prepare-test-env": "node scripts/prepare-test-env.js"
  },
  "keywords": [
    "authentication",
    "security",
    "jwt",
    "express",
    "mongodb",
    "graphql",
    "socket.io"
  ],
  "author": "Swaggo Team",
  "license": "MIT",
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@as-integrations/express5": "^1.1.2",
    "@asamuzakjp/dom-selector": "^6.7.3",
    "@dabh/diagnostics": "^2.0.8",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.1",
    "@graphql-tools/schema": "^9.0.19",
    "@graphql-tools/utils": "^9.2.1",
    "@inversifyjs/common": "^1.5.2",
    "@ioredis/commands": "^1.4.0",
    "argon2": "0.44.0",
    "axios": "1.12.2",
    "bcrypt": "^6.0.0",
    "cheerio": "1.1.2",
    "compression": "1.8.1",
    "cookie-parser": "1.4.7",
    "cors": "2.8.5",
    "csrf": "3.1.0",
    "css-tree": "^3.1.0",
    "cssstyle": "^5.3.1",
    "dataloader": "2.2.3",
    "dayjs": "1.11.18",
    "dompurify": "^3.3.0",
    "dotenv": "^17.2.3",
    "express": "5.1.0",
    "express-rate-limit": "8.1.0",
    "express-validator": "7.2.1",
    "geoip-lite": "1.4.10",
    "graphql": "16.11.0",
    "graphql-depth-limit": "1.1.0",
    "graphql-middleware": "^6.1.35",
    "graphql-query-complexity": "1.1.0",
    "graphql-rate-limit": "^3.3.0",
    "graphql-shield": "^7.6.5",
    "graphql-subscriptions": "3.0.0",
    "graphql-tag": "2.12.6",
    "graphql-validation-complexity": "^0.4.2",
    "helmet": "8.1.0",
    "inversify": "^7.10.2",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "2.28.0",
    "joi": "18.0.1",
    "jsdom": "^27.0.1",
    "jsonwebtoken": "^9.0.2",
    "jws": "^4.0.0",
    "logform": "^2.7.0",
    "mongodb": "6.20.0",
    "mongoose": "8.18.2",
    "multer": "2.0.2",
    "natural": "^8.1.0",
    "node-cache": "5.1.2",
    "node-cron": "3.0.3",
    "nodemailer": "7.0.3",
    "nodemon": "3.1.10",
    "one-time": "^1.0.0",
    "qrcode": "1.5.4",
    "readable-stream": "^3.6.2",
    "redis": "5.8.3",
    "reflect-metadata": "0.2.2",
    "sharp": "0.34.4",
    "socket.io": "4.8.1",
    "spdy": "4.0.2",
    "speakeasy": "2.0.0",
    "stack-trace": "^0.0.10",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.1",
    "triple-beam": "^1.4.1",
    "ts-node": "10.9.2",
    "ua-parser-js": "2.0.6",
    "underscore": "^1.13.7",
    "uuid": "13.0.0",
    "web-push": "3.6.7",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@jest/globals": "30.2.0",
    "@types/jest": "30.0.0",
    "@types/node": "24.7.1",
    "eslint": "9.37.0",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jsdoc": "4.0.0",
    "jsdoc-to-markdown": "9.1.3",
    "lint-staged": "16.2.4",
    "prettier": "3.2.0",
    "rimraf": "6.0.1",
    "supertest": "7.1.4",
    "typescript": "^5.3.0"
  },
  "overrides": {
    "graphql": "16.11.0"
  }
}
